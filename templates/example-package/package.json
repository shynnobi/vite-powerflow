{
  "name": "@vite-powerflow/example-package",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "generate:barrels": "tsx ../../scripts/generate-barrels.ts",
    "prebuild": "pnpm run generate:barrels",
    "build": "pnpm run prebuild && tsx src/build.ts && tsc --emitDeclarationOnly --outDir dist",
    "lint": "eslint . --ext ts,tsx,js,jsx",
    "lint:fix": "eslint . --ext ts,tsx,js,jsx --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "fix": "prettier --write . && eslint . --ext ts,tsx,js,jsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "@vite-powerflow/utils": "workspace:*"
  },
  "devDependencies": {
    "esbuild": "^0.25.6"
  },
  "author": {
    "name": "Shynn",
    "url": "http://github.com/shynnobi"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shynnobi/vite-powerflow.git"
  },
  "homepage": "https://vite-powerflow.netlify.app",
  "bugs": {
    "url": "https://github.com/shynnobi/vite-powerflow/issues"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.13.1"
}
